{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug csharp",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "publish csharp",
      "program": "${workspaceFolder}/code-tutorials/csharp/bin/Debug/net7.0/csharp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/code-tutorials/csharp",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "DAPR_API_TOKEN": "",
        "DAPR_GRPC_ENDPOINT": ""
      },
      "logging": {
        "moduleLoad": false
      }
    },
    {
      "name": "Python: Module",
      "type": "python",
      "cwd": "${workspaceFolder}/code-tutorials/python",
      "env": {
        "DAPR_API_TOKEN": "",
        "DAPR_GRPC_ENDPOINT": "",
        "PYDEVD_DISABLE_FILE_VALIDATION": "1"
      },
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app","--reload"],
      "pythonArgs": ["-Xfrozen_modules=off"]
  }
  ]
}
